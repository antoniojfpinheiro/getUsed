#: kivy 1.10.0

<GetUsedScreen>:

    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'center'

        Image:
            source: '../resources/getused_slogan.png'
            keep_ratio: True
            size_hint_y: None
            size_hint_x: None
            width: self.parent.width
            height: self.parent.width/self.image_ratio